<h1>User#edit</h1>

<%= form_for @user do |f| %>



  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>

  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>

  <div class="field">
    <%= f.submit %>
  </div>
<% end %>
